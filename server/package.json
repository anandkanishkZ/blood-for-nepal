{
  "name": "blood-for-nepal-server",
  "version": "1.0.0",
  "description": "Backend server for Blood For Nepal application",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:reset": "npx sequelize-cli db:migrate:undo:all && npm run db:migrate && npm run db:seed"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "sequelize": "^6.35.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "express-validator": "^7.0.1",
    "express-rate-limit": "^7.1.5",
    "dotenv": "^16.3.1",
    "cookie-parser": "^1.4.6",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2"
  },
  "keywords": [
    "blood-donation",
    "nepal",
    "express",
    "sequelize",
    "postgresql"
  ],
  "author": "Blood For Nepal Team",
  "license": "MIT"
}
